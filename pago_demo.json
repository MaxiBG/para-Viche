{
   "version":"3.1",
   "screens":[
      {
         "id":"PAGO_INICIADO",
         "title":"Comenzar el Pago",
         "layout":{
            "type":"SingleColumnLayout",
            "children":[
               {
                  "type":"Form",
                  "name":"form_payment",
                  "children":[
                     {
                        "type":"Image",
                        "src":"",
                        "height":175
                     },
                     {
                        "type":"TextSubheading",
                        "text":"Ingrese los datos de su tarjeta de Crédito"
                     },
                     {
                        "type":"TextInput",
                        "required":true,
                        "label":"Nombre como figura en la tarjeta",
                        "name":"nombre",
                        "input-type":"text"
                     },
                     {
                        "type":"TextInput",
                        "required":true,
                        "label":"Ingresde el Número de su tarjeta",
                        "name":"numero",
                        "input-type":"text"
                     },
                     {
                        "type":"TextInput",
                        "required":true,
                        "label":"Ingresde la fecha de vencimiento con formato mm/AA",
                        "name":"vencimiento",
                        "input-type":"text"
                     },
                     {
                        "type":"TextInput",
                        "required":true,
                        "label":"Código de Seguridad",
                        "name":"codigo",
                        "input-type":"password"
                     },
                     {
                        "type":"Footer",
                        "label":"Pagar",
                        "on-click-action":{
                           "name":"navigate",
                           "next":{
                              "name":"PAGO_REALIZADO",
                              "type":"screen"
                           },
                           "payload":{
                              "nombre":"${form.nombre}",
                              "codigo":"${form.codigo}"
                           }
                        }
                     }
                  ]
               }
            ]
         }
      },
      {
         "id":"PAGO_REALIZADO",
         "title":"Pagos",
         "data":{
            "rut":{
               "type":"string",
               "__example__":"john.doe"
            },
            "password":{
               "type":"string",
               "__example__":"123abc"
            }
         },
         "terminal":true,
         "layout":{
            "type":"SingleColumnLayout",
            "children":[
               {
                  "type":"Image",
                  "src":"",
                  "width":200,
                  "height":225
               },
               {
                  "type":"TextHeading",
                  "text":"Listo!"
               },
               {
                  "type":"TextSubheading",
                  "text":"Su pago ha sido procesado exitosamente"
               },
               {
                  "type":"TextBody",
                  "text":"Ya puedes continuar."
               },
               {
                  "type":"Footer",
                  "label":"Volver al chat",
                  "on-click-action":{
                     "name":"complete",
                     "payload":{
                        "user":"${data.rut}",
                        "password":"${data.password}"
                     }
                  }
               }
            ]
         }
      }
   ]
}